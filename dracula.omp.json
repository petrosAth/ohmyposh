{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "dr_dark0": "#242631",
    "dr_dark1": "#282a36",
    "dr_dark2": "#363948",
    "dr_dark3": "#44475a",
    "dr_dark4": "#515c83",
    "dr_light0": "#ffffff",
    "dr_light1": "#f8f8f2",
    "dr_light2": "#dadde2",
    "dr_light3": "#adb5cb",
    "dr_light4": "#6272a4",
    "dr_red": "#ff5555",
    "dr_orange": "#ffb86c",
    "dr_yellow": "#f1fa8c",
    "dr_green": "#50fa7b",
    "dr_cyan": "#8be9fd",
    "dr_blue": "#4ea2f1",
    "dr_violet": "#bd93f9",
    "dr_magenta": "#ff79c6"
  },
  "secondary_prompt": {
    "template": " ",
    "foreground": "p:dr_dark3",
    "background": "transparent"
  },
  "transient_prompt": {
    "template": "\u257c[<p:dr_orange>{{ now | date \"\ueab0 01/02\" }}</> \ueab6 <p:dr_orange>{{ now | date \"\ue641 15:04:05\" }}</>]\u257e\u257c[<p:dr_magenta>{{ .pwd }}</>]❱ ",
    "foreground": "p:dr_dark3",
    "background": "transparent"
  },
  "console_title_template": "{{ .folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\u252e",
          "foreground": "p:dr_green",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .code 0 }}p:dr_red{{ end }}"]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<p:dr_dark3>[</>{{.username}}",
          "foreground": "p:dr_blue",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{if .root}}<p:dr_dark3>|</><p:dr_yellow>\uf0e7</><p:dr_dark3>|</>{{else}}<p:dr_dark3>|</>@<p:dr_dark3>|</>{{end}}",
          "foreground": "p:dr_cyan",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{.hostname}}<p:dr_dark3>]</>",
          "foreground": "p:dr_blue",
          "type": "session",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "folder_separator_icon": " \ueab6 ",
            "folder_icon": "...",
            "style": "agnoster_short",
            "max_depth": 4
          },
          "template": "<p:dr_dark3>\u257e\u257c[</>  {{ .path }}<p:dr_dark3>]</>",
          "foreground": "p:dr_magenta",
          "type": "path",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "branch_ahead_icon": "\udb80\udc5d",
            "branch_behind_icon": "\udb80\udc45",
            "branch_icon": "\uea68 ",
            "branch_max_length": 30,
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\ueafc ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true,
            "git_icon": "\udb80\udea2 ",
            "github_icon": "\uea84 ",
            "merge_icon": "\ueafe ",
            "no_commits_icon": "\ueacc",
            "rebase_icon": "\ueafd ",
            "revert_icon": "\ueae2 ",
            "tag_icon": "\uea66 ",
            "truncate_symbol": "\ueab6"
          },
          "template": "<p:dr_dark3>\u257e\u257c[</>{{ .upstreamicon }}{{ .head }} {{ .branchstatus }}{{ if .working.changed }}<p:dr_dark3>|</><p:dr_yellow>\uebb5 </><p:dr_dark3>\ueab6</>{{ if gt .working.added 0 }}<p:dr_green> \uf457 {{ .working.added }}</>{{ end }}{{ if gt .working.deleted 0 }}<p:dr_red> \uf458 {{ .working.deleted }}</>{{ end }}{{ if gt .working.modified 0 }}<p:dr_yellow> \uf459 {{ .working.modified }}</>{{ end }}{{ if gt .working.untracked 0 }}<p:dr_light0> \udb80\udea0 {{ .working.untracked }}</>{{ end }}{{ end }}{{ if .staging.changed }}<p:dr_dark3>|</><p:dr_green>\ueba4 </><p:dr_dark3>\ueab6</>{{ if gt .staging.added 0 }}<p:dr_green> \uf457 {{ .staging.added }}</>{{ end }}{{ if gt .staging.deleted 0 }}<p:dr_red> \uf458 {{ .staging.deleted }}</>{{ end }}{{ if gt .staging.modified 0 }}<p:dr_yellow> \uf459 {{ .staging.modified }}</>{{ end }}{{ end }}{{ if gt .stashcount 0 }}<p:dr_dark3>|</><p:dr_orange>\ueb4b {{.stashcount}}</>{{ end }}{{ if gt .worktreecount 0 }}{{if .isworktree}}<p:dr_dark3>|</><p:dr_orange>\uf1bb {{.worktreecount}}</>{{ end }}{{ end }}<p:dr_dark3>]</>",
          "foreground": "p:dr_violet",
          "type": "git",
          "style": "plain",
          "foreground_templates": [
            "{{ if or (.working.changed) (.staging.changed) }}p:dr_yellow{{ end }}",
            "{{ if and (gt .ahead 0) (gt .behind 0) }}p:dr_yellow{{ end }}",
            "{{ if gt .ahead 0 }}p:dr_green{{ end }}",
            "{{ if gt .behind 0 }}p:dr_red{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\u2515",
          "foreground": "p:dr_green",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .code 0 }}p:dr_red{{ end }}"]
        },
        {
          "properties": {
            "alpine": "\uf300 ",
            "aosc": "\uf301 ",
            "arch": "\uf303 ",
            "centos": "\uf304 ",
            "coreos": "\uf305 ",
            "debian": "\uf306 ",
            "devuan": "\uf307 ",
            "elementary": "\uf309 ",
            "fedora": "\uf30a ",
            "gentoo": "\uf30d ",
            "linux": "\uf31a ",
            "macos": "\uf302 ",
            "mageia": "\uf310 ",
            "manjaro": "\uf312 ",
            "mint": "\uf30e ",
            "nixos": "\uf313 ",
            "opensuse": "\uf314 ",
            "raspbian": "\uf315 ",
            "sabayon": "\uf317 ",
            "slackware": "\uf318 ",
            "ubuntu": "\uf31b ",
            "endeavour": "\uf322 ",
            "windows": "\uf871 ",
            "wsl": "wsl",
            "wsl_separator": " "
          },
          "template": "<p:dr_dark3>[</>{{ if .wsl }}wsl {{ end }}{{.icon}}",
          "foreground": "p:dr_cyan",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<p:dr_dark3>\ueab6 </>\uea85 {{ .name }}",
          "foreground": "p:dr_cyan",
          "type": "shell",
          "style": "plain"
        },
        {
          "style": "plain",
          "template": "<p:dr_dark3> \ueab6 </> {{ .full }}{{ if .error }}{{ .error }}{{ else }}{{ if .venv }}<p:dr_dark3>|</> {{ .venv }}{{ end }}{{ end }}",
          "foreground": "#ffd43b",
          "type": "python"
        },
        {
          "style": "plain",
          "template": "<p:dr_dark3> \ueab6 </> {{ .full }}",
          "foreground": "#339933",
          "type": "node"
        },
        {
          "style": "plain",
          "template": "<p:dr_dark3> \ueab6 </> {{ .full }}",
          "foreground": "#c3002f",
          "type": "angular"
        },
        {
          "style": "plain",
          "template": "<p:dr_dark3> \ueab6 </> {{ .full }}",
          "foreground": "#149eca",
          "type": "react"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:dr_dark3",
          "template": "]"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 2000
          },
          "template": "\u257e\u257c<p:dr_dark3>[</><p:dr_orange>\udb81\udd1b {{ .formattedms }}</><p:dr_dark3>]</>",
          "foreground": "p:dr_green",
          "type": "executiontime",
          "style": "plain",
          "foreground_templates": ["{{ if gt .code 0 }}p:dr_red{{ end }}"]
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "{{ if gt .code 0 }}\u257e\u257c<p:dr_dark3>[</>\udb81\uddad<p:dr_dark3>]</>{{ end }}❱",
          "foreground": "p:dr_green",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .code 0 }}p:dr_red{{ end }}"]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
