{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:draculaGreen",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:draculaRed{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u252e",
          "type": "exit"
        },
        {
          "foreground": "p:draculaPurple",
          "style": "plain",
          "template": "<p:draculaComment>[</>{{.UserName}}",
          "type": "session"
        },
        {
          "foreground": "p:draculaPink",
          "style": "plain",
          "template": "{{if .Root}}<p:draculaComment>|</><p:draculaYellow>\uf0e7</><p:draculaComment>|</>{{else}}<p:draculaComment>|</>@<p:draculaComment>|</>{{end}}",
          "type": "session"
        },
        {
          "foreground": "p:draculaPurple",
          "style": "plain",
          "template": "{{.HostName}}<p:draculaComment>]</>",
          "type": "session"
        },
        {
          "foreground": "p:draculaCyan",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "<p:draculaComment>\u257e\u257c[</>{{ .Path }}<p:draculaComment>]</>",
          "type": "path"
        },
        {
          "foreground": "p:draculaYellow",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:draculaOrange{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:draculaOrange{{ end }}",
            "{{ if gt .Ahead 0 }}p:draculaGreen{{ end }}",
            "{{ if gt .Behind 0 }}p:draculaRed{{ end }}"
          ],
          "properties": {
            "branch_icon": "\uf418 ",
            "branch_max_length": 30,
            "cherry_pick_icon": "CHERRY-PICKING ",
            "commit_icon": "\uf417  ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true,
            "no_commits_icon": "\u2212",
            "rebase_icon": "REBASING ",
            "revert_icon": "REVERTING "
          },
          "style": "plain",
          "template": "<p:draculaComment>\u257e\u257c[</>{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }}<p:draculaComment>|</><p:draculaOrange>\uf044 </>{{ if gt .Working.Added 0 }}<p:draculaGreen> +{{ .Working.Added }}</>{{ end }}{{ if gt .Working.Deleted 0 }}<p:draculaRed> -{{ .Working.Deleted }}</>{{ end }}{{ if gt .Working.Modified 0 }}<p:draculaOrange> ~{{ .Working.Modified }}</>{{ end }}{{ end }}{{ if .Staging.Changed }}<p:draculaComment>|</><p:draculaGreen>\uf046 </>{{ if gt .Staging.Added 0 }}<p:draculaGreen> +{{ .Staging.Added }}</>{{ end }}{{ if gt .Staging.Deleted 0 }}<p:draculaRed> -{{ .Staging.Deleted }}</>{{ end }}{{ if gt .Staging.Modified 0 }}<p:draculaOrange> ~{{ .Staging.Modified }}</>{{ end }}{{ end }}{{ if gt .StashCount 0 }}<p:draculaComment>|</><p:draculaYellow>\uf692 {{.StashCount}}</>{{ end }}{{ if gt .WorktreeCount 0 }}{{if .IsWorkTree}}<p:draculaComment>|</><p:draculaYellow>\uf1bb {{.WorktreeCount}}</>{{ end }}{{ end }}<p:draculaComment>]</>",
          "type": "git"
        },
        {
          "foreground": "#174E9C",
          "style": "plain",
          "template": "<p:draculaComment>\u257e\u257c[</>\ufd31 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<p:draculaComment>]</>",
          "type": "kubectl"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:draculaGreen",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:draculaRed{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u2515",
          "type": "exit"
        },
        {
          "foreground": "p:draculaPink",
          "properties": {
            "alpine": "\uf300 ",
            "aosc": "\uf301 ",
            "arch": "\uf303 ",
            "centos": "\uf304 ",
            "coreos": "\uf305 ",
            "debian": "\uf306 ",
            "devuan": "\uf307 ",
            "elementary": "\uf309 ",
            "fedora": "\uf30a ",
            "gentoo": "\uf30d ",
            "linux": "\uf31a ",
            "macos": "\uf302 ",
            "mageia": "\uf310 ",
            "manjaro": "\uf312 ",
            "mint": "\uf30e ",
            "nixos": "\uf313 ",
            "opensuse": "\uf314 ",
            "raspbian": "\uf315 ",
            "sabayon": "\uf317 ",
            "slackware": "\uf318 ",
            "ubuntu": "\uf31b ",
            "windows": "\uf871 ",
            "wsl": "WSL",
            "wsl_separator": " "
          },
          "style": "plain",
          "template": "<p:draculaComment>[</>{{ if .WSL }}WSL {{ end }}{{.Icon}}<p:draculaComment>|</>",
          "type": "os"
        },
        {
          "foreground": "p:draculaPink",
          "style": "plain",
          "template": "\uf489 {{ .Name }}<p:draculaComment>]</>",
          "type": "shell"
        },
        {
          "foreground": "p:draculaGreen",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:draculaRed{{ end }}"
          ],
          "properties": {
            "style": "austin",
            "threshold": 2000
          },
          "style": "plain",
          "template": "\u257e\u257c<p:draculaComment>[</><p:draculaYellow>\ufa1a{{ .FormattedMs }}</><p:draculaComment>]</>",
          "type": "executiontime"
        },
        {
          "foreground": "p:draculaGreen",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:draculaRed{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "{{ if gt .Code 0 }}\u257e\u257c<p:draculaComment>[</>\uf655<p:draculaComment>]</>{{ end }}\u276f",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "osc99": false,
  "palette": {
    "draculaBg"       : "#282A36",
    "draculaSelection": "#44475A",
    "draculaFg"       : "#F8F8F2",
    "draculaComment"  : "#6272A4",
    "draculaCyan"     : "#8BE9FD",
    "draculaGreen"    : "#50FA7B",
    "draculaOrange"   : "#FFB86C",
    "draculaPink"     : "#FF79C6",
    "draculaPurple"   : "#BD93F9",
    "draculaRed"      : "#FF5555",
    "draculaYellow"   : "#F1FA8C"
  },
  "transient_prompt": {
    "background": "transparent",
    "foreground": "p:draculaComment",
    "template": "\u257c[<p:draculaYellow>{{ now | date \"\uf5ed 01/02\" }}</>|<p:draculaYellow>{{ now | date \"\uf64f 15:04:05\" }}</>]\u257e\u257c[<p:draculaCyan>{{ .PWD }}</>]: "
  },
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "p:draculaComment",
    "template": " "
  },
  "version": 2
}
