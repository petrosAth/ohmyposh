{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:draculaVeryLightMalachiteGreen",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:draculaTomato{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": "┬"
          },
          "style": "plain",
          "type": "exit"
        },
        {
          "foreground": "p:draculaMauve",
          "properties": {
            "template": "<p:draculaWaikawaGrey>[</>{{.UserName}}"
          },
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "p:draculaNeonPink",
          "properties": {
            "template": "{{if .Root}}<p:draculaWaikawaGrey>|</><p:draculaPalePrim></><p:draculaWaikawaGrey>|</>{{else}}<p:draculaWaikawaGrey>|</>@<p:draculaWaikawaGrey>|</>{{end}}"
          },
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "p:draculaMauve",
          "properties": {
            "template": "{{.HostName}}<p:draculaWaikawaGrey>]</>"
          },
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "p:draculaElectricBlue",
          "properties": {
            "style": "full",
            "template": "<p:draculaWaikawaGrey>[</>{{ .Path }}<p:draculaWaikawaGrey>]</>"
          },
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "p:draculaPalePrim",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:draculaMacaroniAndCheese{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:draculaMacaroniAndCheese{{ end }}",
            "{{ if gt .Ahead 0 }}p:draculaVeryLightMalachiteGreen{{ end }}",
            "{{ if gt .Behind 0 }}p:draculaTomato{{ end }}"
          ],
          "properties": {
            "branch_icon": " ",
            "branch_max_length": 30,
            "cherry_pick_icon": "CHERRY-PICKING ",
            "commit_icon": "  ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true,
            "no_commits_icon": "−",
            "rebase_icon": "REBASING ",
            "revert_icon": "REVERTING ",
            "template": "<p:draculaWaikawaGrey>[</>{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }}<p:draculaWaikawaGrey>|</><p:draculaMacaroniAndCheese> </>{{ if gt .Working.Added 0 }}<p:draculaVeryLightMalachiteGreen> +{{ .Working.Added }}</>{{ end }}{{ if gt .Working.Deleted 0 }}<p:draculaTomato> -{{ .Working.Deleted }}</>{{ end }}{{ if gt .Working.Modified 0 }}<p:draculaMacaroniAndCheese> ~{{ .Working.Modified }}</>{{ end }}{{ end }}{{ if .Staging.Changed }}<p:draculaWaikawaGrey>|</><p:draculaVeryLightMalachiteGreen> </>{{ if gt .Staging.Added 0 }}<p:draculaVeryLightMalachiteGreen> +{{ .Staging.Added }}</>{{ end }}{{ if gt .Staging.Deleted 0 }}<p:draculaTomato> -{{ .Staging.Deleted }}</>{{ end }}{{ if gt .Staging.Modified 0 }}<p:draculaMacaroniAndCheese> ~{{ .Staging.Modified }}</>{{ end }}{{ end }}{{ if gt .StashCount 0 }}<p:draculaWaikawaGrey>|</><p:draculaPalePrim> {{.StashCount}}</>{{ end }}{{ if gt .WorktreeCount 0 }}{{if .IsWorkTree}}<p:draculaWaikawaGrey>|</><p:draculaPalePrim> {{.WorktreeCount}}</>{{ end }}{{ end }}<p:draculaWaikawaGrey>]</>"
          },
          "style": "plain",
          "type": "git"
        },
        {
          "foreground": "#174E9C",
          "properties": {
            "template": "<p:draculaWaikawaGrey>[</>ﴱ {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<p:draculaWaikawaGrey>]</>"
          },
          "style": "plain",
          "type": "kubectl"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:draculaVeryLightMalachiteGreen",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:draculaTomato{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": "└"
          },
          "style": "plain",
          "type": "exit"
        },
        {
          "foreground": "p:draculaNeonPink",
          "properties": {
            "alpine": " ",
            "aosc": " ",
            "arch": " ",
            "centos": " ",
            "coreos": " ",
            "debian": " ",
            "devuan": " ",
            "elementary": " ",
            "fedora": " ",
            "gentoo": " ",
            "linux": " ",
            "macos": " ",
            "mageia": " ",
            "manjaro": " ",
            "mint": " ",
            "nixos": " ",
            "opensuse": " ",
            "raspbian": " ",
            "sabayon": " ",
            "slackware": " ",
            "template": "<p:draculaWaikawaGrey>[</>{{ if .WSL }}WSL {{ end }}{{.Icon}}<p:draculaWaikawaGrey>|</>",
            "ubuntu": " ",
            "windows": " ",
            "wsl": "WSL",
            "wsl_separator": " "
          },
          "style": "plain",
          "type": "os"
        },
        {
          "foreground": "p:draculaNeonPink",
          "properties": {
            "template": "\uf489 {{ .Name }}<p:draculaWaikawaGrey>]</>"
          },
          "style": "plain",
          "type": "shell"
        },
        {
          "foreground": "p:draculaPalePrim",
          "properties": {
            "style": "austin",
            "template": "<p:draculaWaikawaGrey>[</>祥{{ .FormattedMs }}<p:draculaWaikawaGrey>]</>",
            "threshold": 2000
          },
          "style": "plain",
          "type": "executiontime"
        },
        {
          "foreground": "p:draculaVeryLightMalachiteGreen",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:draculaTomato{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": "{{ if gt .Code 0 }}<p:draculaWaikawaGrey>[</><p:draculaWaikawaGrey>]</>{{ end }}\uf460"
          },
          "style": "plain",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title": true,
  "console_title_style": "path",
  "final_space": true,
  "osc99": true,
  "palette": {
    "draculaAlabaster": "#F8F8F2",
    "draculaBlackRock": "#282A36",
    "draculaBlueZodiac": "#44475A",
    "draculaElectricBlue": "#8BE9FD",
    "draculaMacaroniAndCheese": "#FFB86C",
    "draculaMauve": "#BD93F9",
    "draculaNeonPink": "#FF79C6",
    "draculaPalePrim": "#F1FA8C",
    "draculaTomato": "#FF5555",
    "draculaVeryLightMalachiteGreen": "#50FA7B",
    "draculaWaikawaGrey": "#6272A4"
  },
  "version": 1
}
