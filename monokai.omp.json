{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "mon_dark0": "#22231d",
    "mon_dark1": "#272822",
    "mon_dark2": "#36362d",
    "mon_dark3": "#454439",
    "mon_dark4": "#5d5a4b",
    "mon_light0": "#ffffff",
    "mon_light1": "#f8f8f2",
    "mon_light2": "#dddcd3",
    "mon_light3": "#c3c1b6",
    "mon_light4": "#75715e",
    "mon_red": "#ff4f4f",
    "mon_orange": "#fd971f",
    "mon_yellow": "#e6db74",
    "mon_green": "#a6e22e",
    "mon_cyan": "#66d9ef",
    "mon_blue": "#5fafff",
    "mon_violet": "#ae81ff",
    "mon_magenta": "#f92672"
  },
  "secondary_prompt": {
    "template": " ",
    "foreground": "p:mon_dark4",
    "background": "transparent"
  },
  "transient_prompt": {
    "template": "\u257c[<p:mon_orange>{{ now | date \"\ueab0 01/02\" }}</> \ueab6 <p:mon_orange>{{ now | date \"\ue641 15:04:05\" }}</>]\u257e\u257c[<p:mon_magenta>{{ .PWD }}</>]❱ ",
    "foreground": "p:mon_dark4",
    "background": "transparent"
  },
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\u252e",
          "foreground": "p:mon_green",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}p:mon_red{{ end }}"]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<p:mon_dark4>[</>{{.UserName}}",
          "foreground": "p:mon_blue",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{if .Root}}<p:mon_dark4>|</><p:mon_yellow>\uf0e7</><p:mon_dark4>|</>{{else}}<p:mon_dark4>|</>@<p:mon_dark4>|</>{{end}}",
          "foreground": "p:mon_cyan",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{.HostName}}<p:mon_dark4>]</>",
          "foreground": "p:mon_blue",
          "type": "session",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "folder_separator_icon": " \ueab6 ",
            "folder_icon": "...",
            "style": "agnoster_short",
            "max_depth": 4
          },
          "template": "<p:mon_dark4>\u257e\u257c[</>  {{ .Path }}<p:mon_dark4>]</>",
          "foreground": "p:mon_magenta",
          "type": "path",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "branch_ahead_icon": "\udb80\udc5d",
            "branch_behind_icon": "\udb80\udc45",
            "branch_icon": "\uea68 ",
            "branch_max_length": 30,
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\ueafc ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true,
            "git_icon": "\udb80\udea2 ",
            "github_icon": "\uea84 ",
            "merge_icon": "\ueafe ",
            "no_commits_icon": "\ueacc",
            "rebase_icon": "\ueafd ",
            "revert_icon": "\ueae2 ",
            "tag_icon": "\uea66 ",
            "truncate_symbol": "\ueab6"
          },
          "template": "<p:mon_dark4>\u257e\u257c[</>{{ .UpstreamIcon }}{{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }}<p:mon_dark4>|</><p:mon_yellow>\uebb5 </><p:mon_dark4>\ueab6</>{{ if gt .Working.Added 0 }}<p:mon_green> \uf457 {{ .Working.Added }}</>{{ end }}{{ if gt .Working.Deleted 0 }}<p:mon_red> \uf458 {{ .Working.Deleted }}</>{{ end }}{{ if gt .Working.Modified 0 }}<p:mon_yellow> \uf459 {{ .Working.Modified }}</>{{ end }}{{ if gt .Working.Untracked 0 }}<p:mon_light0> \udb80\udea0 {{ .Working.Untracked }}</>{{ end }}{{ end }}{{ if .Staging.Changed }}<p:mon_dark4>|</><p:mon_green>\ueba4 </><p:mon_dark4>\ueab6</>{{ if gt .Staging.Added 0 }}<p:mon_green> \uf457 {{ .Staging.Added }}</>{{ end }}{{ if gt .Staging.Deleted 0 }}<p:mon_red> \uf458 {{ .Staging.Deleted }}</>{{ end }}{{ if gt .Staging.Modified 0 }}<p:mon_yellow> \uf459 {{ .Staging.Modified }}</>{{ end }}{{ end }}{{ if gt .StashCount 0 }}<p:mon_dark4>|</><p:mon_orange>\ueb4b {{.StashCount}}</>{{ end }}{{ if gt .WorktreeCount 0 }}<p:mon_dark4>|</><p:mon_orange>\udb81\udc05 {{ .WorktreeCount }}</>{{ end }}{{ if .IsWorkTree }}<p:mon_dark4>|</><p:mon_orange>\udb81\udc05 \uea9e</>{{ end }}<p:mon_dark4>]</>",
          "foreground": "p:mon_violet",
          "type": "git",
          "style": "plain",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:mon_yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:mon_yellow{{ end }}",
            "{{ if gt .Ahead 0 }}p:mon_green{{ end }}",
            "{{ if gt .Behind 0 }}p:mon_red{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\u2515",
          "foreground": "p:mon_green",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}p:mon_red{{ end }}"]
        },
        {
          "properties": {
            "alpine": "\uf300 ",
            "aosc": "\uf301 ",
            "arch": "\uf303 ",
            "centos": "\uf304 ",
            "coreos": "\uf305 ",
            "debian": "\uf306 ",
            "devuan": "\uf307 ",
            "elementary": "\uf309 ",
            "fedora": "\uf30a ",
            "gentoo": "\uf30d ",
            "linux": "\uf31a ",
            "macos": "\uf302 ",
            "mageia": "\uf310 ",
            "manjaro": "\uf312 ",
            "mint": "\uf30e ",
            "nixos": "\uf313 ",
            "opensuse": "\uf314 ",
            "raspbian": "\uf315 ",
            "sabayon": "\uf317 ",
            "slackware": "\uf318 ",
            "ubuntu": "\uf31b ",
            "endeavour": "\uf322 ",
            "windows": "\uf871 ",
            "wsl": "WSL",
            "wsl_separator": " "
          },
          "template": "<p:mon_dark4>[</>{{ if .WSL }}WSL {{ end }}{{.Icon}}",
          "foreground": "p:mon_cyan",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<p:mon_dark4>\ueab6 </>\uea85 {{ .Name }}",
          "foreground": "p:mon_cyan",
          "type": "shell",
          "style": "plain"
        },
        {
          "style": "plain",
          "template": "<p:mon_dark4> \ueab6 </> {{ .Full }}{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}<p:mon_dark4>|</> {{ .Venv }}{{ end }}{{ end }}",
          "foreground": "#FFD43B",
          "type": "python"
        },
        {
          "style": "plain",
          "template": "<p:mon_dark4> \ueab6 </> {{ .Full }}",
          "foreground": "#339933",
          "type": "node"
        },
        {
          "style": "plain",
          "template": "<p:mon_dark4> \ueab6 </> {{ .Full }}",
          "foreground": "#c3002f",
          "type": "angular"
        },
        {
          "style": "plain",
          "template": "<p:mon_dark4> \ueab6 </> {{ .Full }}",
          "foreground": "#149eca",
          "type": "react"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:mon_dark4",
          "template": "]"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 2000
          },
          "template": "\u257e\u257c<p:mon_dark4>[</><p:mon_orange>\udb81\udd1b {{ .FormattedMs }}</><p:mon_dark4>]</>",
          "foreground": "p:mon_green",
          "type": "executiontime",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}p:mon_red{{ end }}"]
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "{{ if gt .Code 0 }}\u257e\u257c<p:mon_dark4>[</>\udb81\uddad<p:mon_dark4>]</>{{ end }}❱",
          "foreground": "p:mon_green",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}p:mon_red{{ end }}"]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
