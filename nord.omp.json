{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:nord14",
          "foreground_templates": ["{{ if gt .Code 0 }}p:nord11{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u252e",
          "type": "exit"
        },
        {
          "foreground": "p:nord9",
          "style": "plain",
          "template": "<p:nord2>[</>{{.UserName}}",
          "type": "session"
        },
        {
          "foreground": "p:nord8",
          "style": "plain",
          "template": "{{if .Root}}<p:nord2>|</><p:nord13>\uf0e7</><p:nord2>|</>{{else}}<p:nord2>|</>@<p:nord2>|</>{{end}}",
          "type": "session"
        },
        {
          "foreground": "p:nord9",
          "style": "plain",
          "template": "{{.HostName}}<p:nord2>]</>",
          "type": "session"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:nord7",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "<p:nord2>\u257e\u257c[</>{{ .Path }}<p:nord2>]</>",
          "type": "path",
          "min_width": 115
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:nord7",
          "properties": {
            "style": "unique"
          },
          "style": "plain",
          "template": "<p:nord2>\u257e\u257c[</>{{ .Path }}<p:nord2>]</>",
          "type": "path",
          "max_width": 114
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:nord10",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:nord13{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:nord13{{ end }}",
            "{{ if gt .Ahead 0 }}p:nord14{{ end }}",
            "{{ if gt .Behind 0 }}p:nord11{{ end }}"
          ],
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true,
            "branch_icon": " ",
            "branch_ahead_icon": "",
            "branch_behind_icon": "",
            "branch_max_length": 30,
            "truncate_symbol": "",
            "commit_icon": " ",
            "tag_icon": " ",
            "rebase_icon": " ",
            "cherry_pick_icon": " ",
            "revert_icon": " ",
            "merge_icon": " ",
            "no_commits_icon": "",
            "github_icon": " ",
            "git_icon": " "
          },
          "style": "plain",
          "template": "<p:nord2>\u257e\u257c[</>{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }}<p:nord2>|</><p:nord13> </><p:nord2></>{{ if gt .Working.Added 0 }}<p:nord14>  {{ .Working.Added }}</>{{ end }}{{ if gt .Working.Deleted 0 }}<p:nord11>  {{ .Working.Deleted }}</>{{ end }}{{ if gt .Working.Modified 0 }}<p:nord13>  {{ .Working.Modified }}</>{{ end }}{{ if gt .Working.Untracked 0 }}<p:nord6>  {{ .Working.Untracked }}</>{{ end }}{{ end }}{{ if .Staging.Changed }}<p:nord2>|</><p:nord14> </><p:nord2></>{{ if gt .Staging.Added 0 }}<p:nord14>  {{ .Staging.Added }}</>{{ end }}{{ if gt .Staging.Deleted 0 }}<p:nord11>  {{ .Staging.Deleted }}</>{{ end }}{{ if gt .Staging.Modified 0 }}<p:nord13>  {{ .Staging.Modified }}</>{{ end }}{{ end }}{{ if gt .StashCount 0 }}<p:nord2>|</><p:nord12> {{.StashCount}}</>{{ end }}{{ if gt .WorktreeCount 0 }}{{if .IsWorkTree}}<p:nord2>|</><p:nord12>\uf1bb {{.WorktreeCount}}</>{{ end }}{{ end }}<p:nord2>]</>",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#174E9C",
          "style": "plain",
          "template": "<p:nord2>\u257e\u257c[</>\ufd31 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<p:nord2>]</>",
          "type": "kubectl"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:nord14",
          "foreground_templates": ["{{ if gt .Code 0 }}p:nord11{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u2515",
          "type": "exit"
        },
        {
          "foreground": "p:nord8",
          "properties": {
            "alpine": "\uf300 ",
            "aosc": "\uf301 ",
            "arch": "\uf303 ",
            "centos": "\uf304 ",
            "coreos": "\uf305 ",
            "debian": "\uf306 ",
            "devuan": "\uf307 ",
            "elementary": "\uf309 ",
            "fedora": "\uf30a ",
            "gentoo": "\uf30d ",
            "linux": "\uf31a ",
            "macos": "\uf302 ",
            "mageia": "\uf310 ",
            "manjaro": "\uf312 ",
            "mint": "\uf30e ",
            "nixos": "\uf313 ",
            "opensuse": "\uf314 ",
            "raspbian": "\uf315 ",
            "sabayon": "\uf317 ",
            "slackware": "\uf318 ",
            "ubuntu": "\uf31b ",
            "windows": "\uf871 ",
            "wsl": "WSL",
            "wsl_separator": " "
          },
          "style": "plain",
          "template": "<p:nord2>[</>{{ if .WSL }}WSL {{ end }}{{.Icon}}<p:nord2>|</>",
          "type": "os"
        },
        {
          "foreground": "p:nord8",
          "style": "plain",
          "template": "\uf489 {{ .Name }}<p:nord2>]</>",
          "type": "shell"
        },
        {
          "foreground": "p:nord14",
          "foreground_templates": ["{{ if gt .Code 0 }}p:nord11{{ end }}"],
          "properties": {
            "style": "austin",
            "threshold": 2000
          },
          "style": "plain",
          "template": "\u257e\u257c<p:nord2>[</><p:nord12>\ufa1a{{ .FormattedMs }}</><p:nord2>]</>",
          "type": "executiontime"
        },
        {
          "foreground": "p:nord14",
          "foreground_templates": ["{{ if gt .Code 0 }}p:nord11{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "{{ if gt .Code 0 }}\u257e\u257c<p:nord2>[</>\uf655<p:nord2>]</>{{ end }}\u2771",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "osc99": false,
  "palette": {
    "nord0": "#2e3440",
    "nord1": "#3b4252",
    "nord2": "#434c5e",
    "nord3": "#4c566a",
    "nord4": "#d8dee9",
    "nord5": "#e5e9f0",
    "nord6": "#eceff4",
    "nord7": "#8fbcbb",
    "nord8": "#88c0d0",
    "nord9": "#81a1c1",
    "nord10": "#5e81ac",
    "nord11": "#bf616a",
    "nord12": "#d08770",
    "nord13": "#ebcb8b",
    "nord14": "#a3be8c",
    "nord15": "#b48ead"
  },
  "transient_prompt": {
    "background": "transparent",
    "foreground": "p:nord2",
    "template": "\u257c[<p:nord12>{{ now | date \"\uf5ed 01/02\" }}</>|<p:nord12>{{ now | date \"\uf64f 15:04:05\" }}</>]\u257e\u257c[<p:nord7>{{ .PWD }}</>]: "
  },
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "p:nord2",
    "template": " "
  },
  "version": 2
}
