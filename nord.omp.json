{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:nordLaurelGreen",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:nordFuzzyWuzzy{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": "┮"
          },
          "style": "plain",
          "type": "exit"
        },
        {
          "foreground": "p:nordDarkPastelBlue",
          "properties": {
            "template": "<p:nordIndependence>[</>{{.UserName}}"
          },
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "p:nordDarkSkyBlue",
          "properties": {
            "template": "{{if .Root}}<p:nordIndependence>|</><p:nordGold></><p:nordIndependence>|</>{{else}}<p:nordIndependence>|</>@<p:nordIndependence>|</>{{end}}"
          },
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "p:nordDarkPastelBlue",
          "properties": {
            "template": "{{.HostName}}<p:nordIndependence>]</>"
          },
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "p:nordPewterBlue",
          "properties": {
            "style": "full",
            "template": "<p:nordIndependence>╾╼[</>{{ .Path }}<p:nordIndependence>]</>"
          },
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "p:nordCopper",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:nordGold{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:nordGold{{ end }}",
            "{{ if gt .Ahead 0 }}p:nordLaurelGreen{{ end }}",
            "{{ if gt .Behind 0 }}p:nordFuzzyWuzzy{{ end }}"
          ],
          "properties": {
            "branch_icon": " ",
            "branch_max_length": 30,
            "cherry_pick_icon": "CHERRY-PICKING ",
            "commit_icon": "  ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true,
            "no_commits_icon": "−",
            "rebase_icon": "REBASING ",
            "revert_icon": "REVERTING ",
            "template": "<p:nordIndependence>╾╼[</>{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }}<p:nordIndependence>|</><p:nordGold> </>{{ if gt .Working.Added 0 }}<p:nordLaurelGreen> +{{ .Working.Added }}</>{{ end }}{{ if gt .Working.Deleted 0 }}<p:nordFuzzyWuzzy> -{{ .Working.Deleted }}</>{{ end }}{{ if gt .Working.Modified 0 }}<p:nordGold> ~{{ .Working.Modified }}</>{{ end }}{{ end }}{{ if .Staging.Changed }}<p:nordIndependence>|</><p:nordLaurelGreen> </>{{ if gt .Staging.Added 0 }}<p:nordLaurelGreen> +{{ .Staging.Added }}</>{{ end }}{{ if gt .Staging.Deleted 0 }}<p:nordFuzzyWuzzy> -{{ .Staging.Deleted }}</>{{ end }}{{ if gt .Staging.Modified 0 }}<p:nordGold> ~{{ .Staging.Modified }}</>{{ end }}{{ end }}{{ if gt .StashCount 0 }}<p:nordIndependence>|</><p:nordCopper> {{.StashCount}}</>{{ end }}{{ if gt .WorktreeCount 0 }}{{if .IsWorkTree}}<p:nordIndependence>|</><p:nordCopper> {{.WorktreeCount}}</>{{ end }}{{ end }}<p:nordIndependence>]</>"
          },
          "style": "plain",
          "type": "git"
        },
        {
          "foreground": "#174E9C",
          "properties": {
            "template": "<p:nordIndependence>╾╼[</>ﴱ {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<p:nordIndependence>]</>"
          },
          "style": "plain",
          "type": "kubectl"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:nordLaurelGreen",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:nordFuzzyWuzzy{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": "┕"
          },
          "style": "plain",
          "type": "exit"
        },
        {
          "foreground": "p:nordDarkSkyBlue",
          "properties": {
            "alpine": " ",
            "aosc": " ",
            "arch": " ",
            "centos": " ",
            "coreos": " ",
            "debian": " ",
            "devuan": " ",
            "elementary": " ",
            "fedora": " ",
            "gentoo": " ",
            "linux": " ",
            "macos": " ",
            "mageia": " ",
            "manjaro": " ",
            "mint": " ",
            "nixos": " ",
            "opensuse": " ",
            "raspbian": " ",
            "sabayon": " ",
            "slackware": " ",
            "template": "<p:nordIndependence>[</>{{ if .WSL }}WSL {{ end }}{{.Icon}}<p:nordIndependence>|</>",
            "ubuntu": " ",
            "windows": " ",
            "wsl": "WSL",
            "wsl_separator": " "
          },
          "style": "plain",
          "type": "os"
        },
        {
          "foreground": "p:nordDarkSkyBlue",
          "properties": {
            "template": " {{ .Name }}<p:nordIndependence>]</>"
          },
          "style": "plain",
          "type": "shell"
        },
        {
          "foreground": "p:nordLaurelGreen",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:nordFuzzyWuzzy{{ end }}"
          ],
          "properties": {
            "style": "austin",
            "template": "╾╼<p:nordIndependence>[</><p:nordCopper>祥{{ .FormattedMs }}</><p:nordIndependence>]</>",
            "threshold": 2000
          },
          "style": "plain",
          "type": "executiontime"
        },
        {
          "foreground": "p:nordLaurelGreen",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:nordFuzzyWuzzy{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": "{{ if gt .Code 0 }}╾╼<p:nordIndependence>[</><p:nordIndependence>]</>{{ end }}❯ "
          },
          "style": "plain",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "p:nordCopper",
    "template": "<p:nordIndependence>╼[</>{{ now | date \" 01-02<p:nordIndependence>|</> 15:04:05\" }}<p:nordIndependence>]❯</> "
  },
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{ .Folder }}",
  "final_space": false,
  "osc99": true,
  "palette": {
    "nordBrightGray": "#e5e9f0",
    "nordCharcoal": "#3b4252",
    "nordCopper": "#d08770",
    "nordCultured": "#d8dee9",
    "nordDarkPastelBlue": "#81a1c1",
    "nordDarkSkyBlue": "#88c0d0",
    "nordFuzzyWuzzy": "#bf616a",
    "nordGlossyGrape": "#b48ead",
    "nordGold": "#ebcb8b",
    "nordIndependence": "#4c566a",
    "nordIsabelline": "#eceff4",
    "nordLaurelGreen": "#a3be8c",
    "nordOuterSpace": "#2e3440",
    "nordPewterBlue": "#8fbcbb",
    "nordPoliceBlue": "#434c5e",
    "nordRackley": "#5e81ac"
  },
  "version": 1
}
