{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:nord14",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:nord11{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": "┮"
          },
          "style": "plain",
          "type": "exit"
        },
        {
          "foreground": "p:nord9",
          "properties": {
            "template": "<p:nord2>[</>{{.UserName}}"
          },
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "p:nord8",
          "properties": {
            "template": "{{if .Root}}<p:nord2>|</><p:nord13></><p:nord2>|</>{{else}}<p:nord2>|</>@<p:nord2>|</>{{end}}"
          },
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "p:nord9",
          "properties": {
            "template": "{{.HostName}}<p:nord2>]</>"
          },
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "p:nord7",
          "properties": {
            "style": "full",
            "template": "<p:nord2>╾╼[</>{{ .Path }}<p:nord2>]</>"
          },
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "p:nord10",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:nord13{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:nord13{{ end }}",
            "{{ if gt .Ahead 0 }}p:nord14{{ end }}",
            "{{ if gt .Behind 0 }}p:nord11{{ end }}"
          ],
          "properties": {
            "branch_icon": " ",
            "branch_max_length": 30,
            "cherry_pick_icon": "CHERRY-PICKING ",
            "commit_icon": "  ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true,
            "no_commits_icon": "−",
            "rebase_icon": "REBASING ",
            "revert_icon": "REVERTING ",
            "template": "<p:nord2>╾╼[</>{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }}<p:nord2>|</><p:nord13> </>{{ if gt .Working.Added 0 }}<p:nord14> +{{ .Working.Added }}</>{{ end }}{{ if gt .Working.Deleted 0 }}<p:nord11> -{{ .Working.Deleted }}</>{{ end }}{{ if gt .Working.Modified 0 }}<p:nord13> ~{{ .Working.Modified }}</>{{ end }}{{ end }}{{ if .Staging.Changed }}<p:nord2>|</><p:nord14> </>{{ if gt .Staging.Added 0 }}<p:nord14> +{{ .Staging.Added }}</>{{ end }}{{ if gt .Staging.Deleted 0 }}<p:nord11> -{{ .Staging.Deleted }}</>{{ end }}{{ if gt .Staging.Modified 0 }}<p:nord13> ~{{ .Staging.Modified }}</>{{ end }}{{ end }}{{ if gt .StashCount 0 }}<p:nord2>|</><p:nord12> {{.StashCount}}</>{{ end }}{{ if gt .WorktreeCount 0 }}{{if .IsWorkTree}}<p:nord2>|</><p:nord12> {{.WorktreeCount}}</>{{ end }}{{ end }}<p:nord2>]</>"
          },
          "style": "plain",
          "type": "git"
        },
        {
          "foreground": "#174E9C",
          "properties": {
            "template": "<p:nord2>╾╼[</>ﴱ {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<p:nord2>]</>"
          },
          "style": "plain",
          "type": "kubectl"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:nord14",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:nord11{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": "┕"
          },
          "style": "plain",
          "type": "exit"
        },
        {
          "foreground": "p:nord8",
          "properties": {
            "alpine": " ",
            "aosc": " ",
            "arch": " ",
            "centos": " ",
            "coreos": " ",
            "debian": " ",
            "devuan": " ",
            "elementary": " ",
            "fedora": " ",
            "gentoo": " ",
            "linux": " ",
            "macos": " ",
            "mageia": " ",
            "manjaro": " ",
            "mint": " ",
            "nixos": " ",
            "opensuse": " ",
            "raspbian": " ",
            "sabayon": " ",
            "slackware": " ",
            "template": "<p:nord2>[</>{{ if .WSL }}WSL {{ end }}{{.Icon}}<p:nord2>|</>",
            "ubuntu": " ",
            "windows": " ",
            "wsl": "WSL",
            "wsl_separator": " "
          },
          "style": "plain",
          "type": "os"
        },
        {
          "foreground": "p:nord8",
          "properties": {
            "template": " {{ .Name }}<p:nord2>]</>"
          },
          "style": "plain",
          "type": "shell"
        },
        {
          "foreground": "p:nord14",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:nord11{{ end }}"
          ],
          "properties": {
            "style": "austin",
            "template": "╾╼<p:nord2>[</><p:nord12>祥{{ .FormattedMs }}</><p:nord2>]</>",
            "threshold": 2000
          },
          "style": "plain",
          "type": "executiontime"
        },
        {
          "foreground": "p:nord14",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:nord11{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": "{{ if gt .Code 0 }}╾╼<p:nord2>[</><p:nord2>]</>{{ end }} "
          },
          "style": "plain",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "p:nord2",
    "template": "╼[<p:nord12>{{ now | date \" 02\" }}</>|<p:nord12>{{ now | date \" 15:04:05\" }}</>]╾╼[<p:nord7>{{ .PWD }}</>]: "
  },
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{ .Folder }}",
  "final_space": false,
  "osc99": true,
  "palette": {
    "nord0":  "#2e3440",
    "nord1":  "#3b4252",
    "nord2":  "#434c5e",
    "nord3":  "#4c566a",
    "nord4":  "#d8dee9",
    "nord5":  "#e5e9f0",
    "nord6":  "#eceff4",
    "nord7":  "#8fbcbb",
    "nord8":  "#88c0d0",
    "nord9":  "#81a1c1",
    "nord10": "#5e81ac",
    "nord11": "#bf616a",
    "nord12": "#d08770",
    "nord13": "#ebcb8b",
    "nord14": "#a3be8c",
    "nord15": "#b48ead"
  },
  "version": 1
}
