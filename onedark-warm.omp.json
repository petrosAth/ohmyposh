{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "odw_dark0": "#191a1c",
    "odw_dark1": "#2c2d30",
    "odw_dark2": "#35373b",
    "odw_dark3": "#3e4045",
    "odw_dark4": "#404247",
    "odw_light0": "#646568",
    "odw_light1": "#8b8d91",
    "odw_light2": "#b1b4b9",
    "odw_red": "#e16d77",
    "odw_orange": "#c99a6e",
    "odw_yellow": "#dfbe81",
    "odw_green": "#99bc80",
    "odw_cyan": "#5fafb9",
    "odw_blue": "#68aee8",
    "odw_violet": "#c27fd7",
    "odw_magenta": "#d6749a"
  },
  "secondary_prompt": {
    "template": " ",
    "foreground": "p:odw_light0",
    "background": "transparent"
  },
  "transient_prompt": {
    "template": "\u257c[<p:odw_orange>{{ now | date \"\ueab0 01/02\" }}</> \ueab6 <p:odw_orange>{{ now | date \"\ue641 15:04:05\" }}</>]\u257e\u257c[<p:odw_magenta>{{ .PWD }}</>]❱ ",
    "foreground": "p:odw_light0",
    "background": "transparent"
  },
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\u252e",
          "foreground": "p:odw_green",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}p:odw_red{{ end }}"]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<p:odw_light0>[</>{{.UserName}}",
          "foreground": "p:odw_blue",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{if .Root}}<p:odw_light0>|</><p:odw_yellow>\uf0e7</><p:odw_light0>|</>{{else}}<p:odw_light0>|</>@<p:odw_light0>|</>{{end}}",
          "foreground": "p:odw_cyan",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{.HostName}}<p:odw_light0>]</>",
          "foreground": "p:odw_blue",
          "type": "session",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "folder_separator_icon": " \ueab6 ",
            "folder_icon": "...",
            "style": "agnoster_short",
            "max_depth": 4
          },
          "template": "<p:odw_light0>\u257e\u257c[</>  {{ .Path }}<p:odw_light0>]</>",
          "foreground": "p:odw_magenta",
          "type": "path",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "branch_ahead_icon": "\udb80\udc5d",
            "branch_behind_icon": "\udb80\udc45",
            "branch_icon": "\uea68 ",
            "branch_max_length": 30,
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\ueafc ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true,
            "git_icon": "\udb80\udea2 ",
            "github_icon": "\uea84 ",
            "merge_icon": "\ueafe ",
            "no_commits_icon": "\ueacc",
            "rebase_icon": "\ueafd ",
            "revert_icon": "\ueae2 ",
            "tag_icon": "\uea66 ",
            "truncate_symbol": "\ueab6"
          },
          "template": "<p:odw_light0>\u257e\u257c[</>{{ .UpstreamIcon }}{{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }}<p:odw_light0>|</><p:odw_yellow>\uebb5 </><p:odw_light0>\ueab6</>{{ if gt .Working.Added 0 }}<p:odw_green> \uf457 {{ .Working.Added }}</>{{ end }}{{ if gt .Working.Deleted 0 }}<p:odw_red> \uf458 {{ .Working.Deleted }}</>{{ end }}{{ if gt .Working.Modified 0 }}<p:odw_yellow> \uf459 {{ .Working.Modified }}</>{{ end }}{{ if gt .Working.Untracked 0 }}<p:odw_light2> \udb80\udea0 {{ .Working.Untracked }}</>{{ end }}{{ end }}{{ if .Staging.Changed }}<p:odw_light0>|</><p:odw_green>\ueba4 </><p:odw_light0>\ueab6</>{{ if gt .Staging.Added 0 }}<p:odw_green> \uf457 {{ .Staging.Added }}</>{{ end }}{{ if gt .Staging.Deleted 0 }}<p:odw_red> \uf458 {{ .Staging.Deleted }}</>{{ end }}{{ if gt .Staging.Modified 0 }}<p:odw_yellow> \uf459 {{ .Staging.Modified }}</>{{ end }}{{ end }}{{ if gt .StashCount 0 }}<p:odw_light0>|</><p:odw_orange>\ueb4b {{.StashCount}}</>{{ end }}{{ if gt .WorktreeCount 0 }}{{if .IsWorkTree}}<p:odw_light0>|</><p:odw_orange>\uf1bb {{.WorktreeCount}}</>{{ end }}{{ end }}<p:odw_light0>]</>",
          "foreground": "p:odw_violet",
          "type": "git",
          "style": "plain",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:odw_yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:odw_yellow{{ end }}",
            "{{ if gt .Ahead 0 }}p:odw_green{{ end }}",
            "{{ if gt .Behind 0 }}p:odw_red{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\u2515",
          "foreground": "p:odw_green",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}p:odw_red{{ end }}"]
        },
        {
          "properties": {
            "alpine": "\uf300 ",
            "aosc": "\uf301 ",
            "arch": "\uf303 ",
            "centos": "\uf304 ",
            "coreos": "\uf305 ",
            "debian": "\uf306 ",
            "devuan": "\uf307 ",
            "elementary": "\uf309 ",
            "fedora": "\uf30a ",
            "gentoo": "\uf30d ",
            "linux": "\uf31a ",
            "macos": "\uf302 ",
            "mageia": "\uf310 ",
            "manjaro": "\uf312 ",
            "mint": "\uf30e ",
            "nixos": "\uf313 ",
            "opensuse": "\uf314 ",
            "raspbian": "\uf315 ",
            "sabayon": "\uf317 ",
            "slackware": "\uf318 ",
            "ubuntu": "\uf31b ",
            "endeavour": "\uf322 ",
            "windows": "\uf871 ",
            "wsl": "WSL",
            "wsl_separator": " "
          },
          "template": "<p:odw_light0>[</>{{ if .WSL }}WSL {{ end }}{{.Icon}}",
          "foreground": "p:odw_cyan",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<p:odw_light0>\ueab6 </>\uea85 {{ .Name }}",
          "foreground": "p:odw_cyan",
          "type": "shell",
          "style": "plain"
        },
        {
          "style": "plain",
          "template": "<p:odw_light0> \ueab6 </> {{ .Full }}{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}<p:odw_light0>|</> {{ .Venv }}{{ end }}{{ end }}",
          "foreground": "#FFD43B",
          "type": "python"
        },
        {
          "style": "plain",
          "template": "<p:odw_light0> \ueab6 </> {{ .Full }}",
          "foreground": "#339933",
          "type": "node"
        },
        {
          "style": "plain",
          "template": "<p:odw_light0> \ueab6 </> {{ .Full }}",
          "foreground": "#c3002f",
          "type": "angular"
        },
        {
          "style": "plain",
          "template": "<p:odw_light0> \ueab6 </> {{ .Full }}",
          "foreground": "#149eca",
          "type": "react"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:odw_light0",
          "template": "]"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 2000
          },
          "template": "\u257e\u257c<p:odw_light0>[</><p:odw_orange>\udb81\udd1b {{ .FormattedMs }}</><p:odw_light0>]</>",
          "foreground": "p:odw_green",
          "type": "executiontime",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}p:odw_red{{ end }}"]
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "{{ if gt .Code 0 }}\u257e\u257c<p:odw_light0>[</>\udb81\uddad<p:odw_light0>]</>{{ end }}❱",
          "foreground": "p:odw_green",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}p:odw_red{{ end }}"]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
