{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "odwr_dark0": "#101012",
    "odwr_dark1": "#232326",
    "odwr_dark2": "#2c2d31",
    "odwr_dark3": "#35363b",
    "odwr_dark4": "#37383d",
    "odwr_light0": "#5a5b5e",
    "odwr_light1": "#818387",
    "odwr_light2": "#a7aab0",
    "odwr_red": "#de5d68",
    "odwr_orange": "#c49060",
    "odwr_yellow": "#dbb671",
    "odwr_green": "#8fb573",
    "odwr_cyan": "#51a8b3",
    "odwr_blue": "#57a5e5",
    "odwr_violet": "#bb70d2",
    "odwr_magenta": "#d1648f"
  },
  "secondary_prompt": {
    "template": " ",
    "foreground": "p:odwr_light0",
    "background": "transparent"
  },
  "transient_prompt": {
    "template": "\u257c[<p:odwr_orange>{{ now | date \"\ueab0 01/02\" }}</> \ueab6 <p:odwr_orange>{{ now | date \"\ue641 15:04:05\" }}</>]\u257e\u257c[<p:odwr_magenta>{{ .PWD }}</>]❱ ",
    "foreground": "p:odwr_light0",
    "background": "transparent"
  },
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\u252e",
          "foreground": "p:odwr_green",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}p:odwr_red{{ end }}"]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<p:odwr_light0>[</>{{.UserName}}",
          "foreground": "p:odwr_blue",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{if .Root}}<p:odwr_light0>|</><p:odwr_yellow>\uf0e7</><p:odwr_light0>|</>{{else}}<p:odwr_light0>|</>@<p:odwr_light0>|</>{{end}}",
          "foreground": "p:odwr_cyan",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{.HostName}}<p:odwr_light0>]</>",
          "foreground": "p:odwr_blue",
          "type": "session",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "folder_separator_icon": " \ueab6 ",
            "folder_icon": "...",
            "style": "agnoster_short",
            "max_depth": 4
          },
          "template": "<p:odwr_light0>\u257e\u257c[</>  {{ .Path }}<p:odwr_light0>]</>",
          "foreground": "p:odwr_magenta",
          "type": "path",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "branch_ahead_icon": "\udb80\udc5d",
            "branch_behind_icon": "\udb80\udc45",
            "branch_icon": "\uea68 ",
            "branch_max_length": 30,
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\ueafc ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true,
            "git_icon": "\udb80\udea2 ",
            "github_icon": "\uea84 ",
            "merge_icon": "\ueafe ",
            "no_commits_icon": "\ueacc",
            "rebase_icon": "\ueafd ",
            "revert_icon": "\ueae2 ",
            "tag_icon": "\uea66 ",
            "truncate_symbol": "\ueab6"
          },
          "template": "<p:odwr_light0>\u257e\u257c[</>{{ .UpstreamIcon }}{{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }}<p:odwr_light0>|</><p:odwr_yellow>\uebb5 </><p:odwr_light0>\ueab6</>{{ if gt .Working.Added 0 }}<p:odwr_green> \uf457 {{ .Working.Added }}</>{{ end }}{{ if gt .Working.Deleted 0 }}<p:odwr_red> \uf458 {{ .Working.Deleted }}</>{{ end }}{{ if gt .Working.Modified 0 }}<p:odwr_yellow> \uf459 {{ .Working.Modified }}</>{{ end }}{{ if gt .Working.Untracked 0 }}<p:odwr_light2> \udb80\udea0 {{ .Working.Untracked }}</>{{ end }}{{ end }}{{ if .Staging.Changed }}<p:odwr_light0>|</><p:odwr_green>\ueba4 </><p:odwr_light0>\ueab6</>{{ if gt .Staging.Added 0 }}<p:odwr_green> \uf457 {{ .Staging.Added }}</>{{ end }}{{ if gt .Staging.Deleted 0 }}<p:odwr_red> \uf458 {{ .Staging.Deleted }}</>{{ end }}{{ if gt .Staging.Modified 0 }}<p:odwr_yellow> \uf459 {{ .Staging.Modified }}</>{{ end }}{{ end }}{{ if gt .StashCount 0 }}<p:odwr_light0>|</><p:odwr_orange>\ueb4b {{.StashCount}}</>{{ end }}{{ if gt .WorktreeCount 0 }}{{if .IsWorkTree}}<p:odwr_light0>|</><p:odwr_orange>\uf1bb {{.WorktreeCount}}</>{{ end }}{{ end }}<p:odwr_light0>]</>",
          "foreground": "p:odwr_violet",
          "type": "git",
          "style": "plain",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:odwr_yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:odwr_yellow{{ end }}",
            "{{ if gt .Ahead 0 }}p:odwr_green{{ end }}",
            "{{ if gt .Behind 0 }}p:odwr_red{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\u2515",
          "foreground": "p:odwr_green",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}p:odwr_red{{ end }}"]
        },
        {
          "properties": {
            "alpine": "\uf300 ",
            "aosc": "\uf301 ",
            "arch": "\uf303 ",
            "centos": "\uf304 ",
            "coreos": "\uf305 ",
            "debian": "\uf306 ",
            "devuan": "\uf307 ",
            "elementary": "\uf309 ",
            "fedora": "\uf30a ",
            "gentoo": "\uf30d ",
            "linux": "\uf31a ",
            "macos": "\uf302 ",
            "mageia": "\uf310 ",
            "manjaro": "\uf312 ",
            "mint": "\uf30e ",
            "nixos": "\uf313 ",
            "opensuse": "\uf314 ",
            "raspbian": "\uf315 ",
            "sabayon": "\uf317 ",
            "slackware": "\uf318 ",
            "ubuntu": "\uf31b ",
            "endeavour": "\uf322 ",
            "windows": "\uf871 ",
            "wsl": "WSL",
            "wsl_separator": " "
          },
          "template": "<p:odwr_light0>[</>{{ if .WSL }}WSL {{ end }}{{.Icon}}",
          "foreground": "p:odwr_cyan",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<p:odwr_light0>\ueab6 </>\uea85 {{ .Name }}",
          "foreground": "p:odwr_cyan",
          "type": "shell",
          "style": "plain"
        },
        {
          "style": "plain",
          "template": "<p:odwr_light0> \ueab6 </> {{ .Full }}",
          "foreground": "#FFD43B",
          "type": "python"
        },
        {
          "style": "plain",
          "template": "<p:odwr_light0> \ueab6 </> {{ .Full }}",
          "foreground": "#339933",
          "type": "node"
        },
        {
          "style": "plain",
          "template": "<p:odwr_light0> \ueab6 </> {{ .Full }}",
          "foreground": "#c3002f",
          "type": "angular"
        },
        {
          "style": "plain",
          "template": "<p:odwr_light0> \ueab6 </> {{ .Full }}",
          "foreground": "#149eca",
          "type": "react"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:odwr_light0",
          "template": "]"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 2000
          },
          "template": "\u257e\u257c<p:odwr_light0>[</><p:odwr_orange>\udb81\udd1b {{ .FormattedMs }}</><p:odwr_light0>]</>",
          "foreground": "p:odwr_green",
          "type": "executiontime",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}p:odwr_red{{ end }}"]
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "{{ if gt .Code 0 }}\u257e\u257c<p:odwr_light0>[</>\udb81\uddad<p:odwr_light0>]</>{{ end }}❱",
          "foreground": "p:odwr_green",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}p:odwr_red{{ end }}"]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
