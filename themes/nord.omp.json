{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:nordLaurelGreen",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:nordFuzzyWuzzy{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": "\u252c"
          },
          "style": "plain",
          "type": "exit"
        },
        {
          "foreground": "p:nordPewterBlue",
          "properties": {
            "template": "<p:nordIndependence>[</>{{.UserName}}"
          },
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "p:nordDarkSkyBlue",
          "properties": {
            "template": "{{if .Root}}<p:nordIndependence>|</><p:nordGold>\uf0e7</><p:nordIndependence>|</>{{else}}<p:nordIndependence>|</>@<p:nordIndependence>|</>{{end}}"
          },
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "p:nordPewterBlue",
          "properties": {
            "template": "{{.HostName}}<p:nordIndependence>]</>"
          },
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "p:nordDarkPastelBlue",
          "properties": {
            "style": "full",
            "template": "<p:nordIndependence>[</>{{ .Path }}<p:nordIndependence>]</>"
          },
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "p:nordGold",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:nordCopper{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:nordCopper{{ end }}",
            "{{ if gt .Ahead 0 }}p:nordLaurelGreen{{ end }}",
            "{{ if gt .Behind 0 }}p:nordFuzzyWuzzy{{ end }}"
          ],
          "properties": {
            "branch_icon": "\uf418 ",
            "branch_max_length": 30,
            "cherry_pick_icon": "CHERRY-PICKING ",
            "commit_icon": "\uf417  ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true,
            "no_commits_icon": "\u2212",
            "rebase_icon": "REBASING ",
            "revert_icon": "REVERTING ",
            "template": "<p:nordIndependence>[</>{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }}<p:nordIndependence>|</><p:nordCopper>\uf044 </>{{ if gt .Working.Added 0 }}<p:nordLaurelGreen>+{{ .Working.Added }}</>{{ end }}{{ if gt .Working.Deleted 0 }}<p:nordFuzzyWuzzy> -{{ .Working.Deleted }}</>{{ end }}{{ if gt .Working.Modified 0 }}<p:nordCopper> ~{{ .Working.Modified }}</>{{ end }}{{ end }}{{ if .Staging.Changed }}<p:nordIndependence>|</><p:nordLaurelGreen>\uf046  {{.Staging.String}}</>{{ end }}{{ if gt .StashCount 0 }}<p:nordIndependence>|</><p:nordGold>\uf692 {{.StashCount}}</>{{ end }}{{ if gt .WorktreeCount 0 }}{{if .IsWorkTree}}<p:nordIndependence>|</><p:nordGold>\uf1bb {{.WorktreeCount}}</>{{ end }}{{ end }}<p:nordIndependence>]</>"
          },
          "style": "plain",
          "type": "git"
        },
        {
          "foreground": "#174E9C",
          "properties": {
            "template": "<p:nordIndependence>[</>\ufd31 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<p:nordIndependence>]</>"
          },
          "style": "plain",
          "type": "kubectl"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:nordLaurelGreen",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:nordFuzzyWuzzy{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": "\u2514"
          },
          "style": "plain",
          "type": "exit"
        },
        {
          "foreground": "p:nordDarkSkyBlue",
          "properties": {
            "alpine": "\uf300 ",
            "aosc": "\uf301 ",
            "arch": "\uf303 ",
            "centos": "\uf304 ",
            "coreos": "\uf305 ",
            "debian": "\uf306 ",
            "devuan": "\uf307 ",
            "elementary": "\uf309 ",
            "fedora": "\uf30a ",
            "gentoo": "\uf30d ",
            "linux": "\uf31a ",
            "macos": "\uf302 ",
            "mageia": "\uf310 ",
            "manjaro": "\uf312 ",
            "mint": "\uf30e ",
            "nixos": "\uf313 ",
            "opensuse": "\uf314 ",
            "raspbian": "\uf315 ",
            "sabayon": "\uf317 ",
            "slackware": "\uf318 ",
            "template": "<p:nordIndependence>[</>{{ if .WSL }}WSL {{ end }}{{.Icon}}<p:nordIndependence>|</>",
            "ubuntu": "\uf31b ",
            "windows": "\uf871 ",
            "wsl": "WSL",
            "wsl_separator": " "
          },
          "style": "plain",
          "type": "os"
        },
        {
          "foreground": "p:nordDarkSkyBlue",
          "properties": {
            "template": "\uf489 {{ .Name }}<p:nordIndependence>]</>"
          },
          "style": "plain",
          "type": "shell"
        },
        {
          "foreground": "p:nordGold",
          "properties": {
            "style": "austin",
            "template": "<p:nordIndependence>[</>\ufa1a{{ .FormattedMs }}<p:nordIndependence>]</>",
            "threshold": 2000
          },
          "style": "plain",
          "type": "executiontime"
        },
        {
          "foreground": "p:nordLaurelGreen",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:nordFuzzyWuzzy{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": "\u276f"
          },
          "style": "plain",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title": true,
  "console_title_style": "path",
  "final_space": true,
  "osc99": true,
  "palette": {
    "nordBrightGray": "#e5e9f0",
    "nordCharcoal": "#3b4252",
    "nordCopper": "#d08770",
    "nordCultured": "#d8dee9",
    "nordDarkPastelBlue": "#81a1c1",
    "nordDarkSkyBlue": "#88c0d0",
    "nordFuzzyWuzzy": "#bf616a",
    "nordGlossyGrape": "#b48ead",
    "nordGold": "#ebcb8b",
    "nordIndependence": "#4c566a",
    "nordIsabelline": "#eceff4",
    "nordLaurelGreen": "#a3be8c",
    "nordOuterSpace": "#2e3440",
    "nordPewterBlue": "#8fbcbb",
    "nordPoliceBlue": "#434c5e",
    "nordRackley": "#5e81ac"
  },
  "version": 1
}
